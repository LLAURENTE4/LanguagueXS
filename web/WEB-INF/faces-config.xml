<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{service.login}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/login_user.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{service.listPeople}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/list_people.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{service.listLevels}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/list_levels.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{service.listLessonStudents}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/list_lesson_students.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{service.listSkills}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/list_teachers.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{service.listLanguages}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/list_languages.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>